//Write a single integer to output, denoting how many integers ti are divisible by k. 
#include<stdio.h>
int main()
{
        int n,count=0,x;
        long unsigned t;
        scanf("%d",&n);
        scanf("%d",&x);
        for(int i=0;i<n;i++)
        {
              scanf("%lu",&t);
              if(t%x==0)
              count++;
        }
        printf("%d",count);
        return 0;

}

