#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	int n;
	scanf("%d",&t);
	for(int i=0;i<t;i++)
	{int j;
	        scanf("%d",&n);
	        for(j=2;j<=n;j++)
	        {
	                if(n%j==0)
	                break;
	        }
	        
	        if(n==j)
	        printf("yes");
	        else
	        printf("no");
	        
	        printf("\n");
	        }
	return 0;
}


