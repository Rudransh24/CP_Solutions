#include<bits/stdc++.h>
using namespace std;
#define M 1000000007
#define int long long

int32_t main(void)
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,count=0;
        cin>>n;
        int ar[n];
        for(int i=0;i<n;i++)
        cin>>ar[i];
        int ans=0;
        for(int i=0;i<n;i++)
        {
            if(ar[i]%2==0)
            {
                count++;
            }
            else
            ans+=count;
        }
        cout<<ans<<endl;
    }

    return 0;
}
