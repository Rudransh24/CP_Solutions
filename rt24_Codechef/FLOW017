#include <stdio.h>

int main(void) {
    unsigned long a,b,c;
    int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
            scanf("%lu %lu %lu",&a,&b,&c);
            if(a>b && b>c)
            printf("%lu",b);
            else
             if(a>b && b<c && a>c)
            printf("%lu",c);
            else
             if(a>b && b<c && a<c)
            printf("%lu",a);
            else
             if(a<b && b>c && a>c)
            printf("%lu",a);
            else
             if(a<b && b>c && a<c)
            printf("%lu",c);
            else
             if(a<b && b<c)
            printf("%lu",b);
            else
                continue;
            
            printf("\n");
            
            
    }
	return 0;
}


