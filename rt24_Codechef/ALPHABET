#include<bits/stdc++.h>
using namespace std;
#define M 1000000007

int main(void)
{
    string x;
    cin>>x;
    int t;
    cin>>t;
    while(t--)
    {
        string y;
        cin>>y;
        int count=0;
        for(int i=0;i<x.size();i++)
        {
            for(int j=0;j<y.size();j++)
            {
                if(x[i]==y[j])
                count++;
                else
                continue;
            }
        }
        if(count==y.size())
        cout<<"Yes"<<endl;
        else
        cout<<"No"<<endl;
    }
    return 0;
    
}
