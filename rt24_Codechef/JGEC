#include<bits/stdc++.h>
using namespace std;
#define M 1000000007
#define int long long

int32_t main(void)
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        char c[n];
        for(int i=0;i<n;i++)
        cin>>c[i];
        int count=0;
        for(int i=0;i<n;i++)
        {
            if(c[i]=='J'&&c[i+1]=='G'&&c[i+2]=='E'&&c[i+3]=='C')
            count++;
            else
            continue;
        }
        cout<<count<<endl;
    }

    return 0;
}

