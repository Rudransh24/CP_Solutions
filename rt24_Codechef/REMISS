#include <stdio.h>

int main(void) {
	int a,b,t;scanf("%d",&t);
	for(int i=0;i<t;i++)
	{       int max,min;
	        scanf("%d %d",&a,&b);
	        max=a+b;
	        if(a>b)
	         min=a;
	        else
	        min=b;
            printf("%d %d",min,max);	        
	       printf("\n");       
	}
	return 0;
}


