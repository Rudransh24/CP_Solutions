#include<bits/stdc++.h>
using namespace std;
#define M 1000000007
#define pi 3.1415926535897932384626433832
#define x 0.000001
typedef float ll;

int main(void)
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int ar[n];
        for(int i=0;i<n;i++)
        cin>>ar[i];
        ll sum1=0;
        ll sum=accumulate(ar, ar+n, sum1);
        ll p=sum/(10*n*pi);
        cout<<p<<endl;
        
    }    
    return 0;
}
