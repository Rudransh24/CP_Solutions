#include <stdio.h>

int main(void) {
	// your code goes here
	int t,a,b,c;
	scanf("%d",&t);
	for(int i=0;i<t;i++)
	{int sum=0;
	        scanf("%d %d %d", &a,&b,&c);
	        sum=a+b+c;
	        if(sum==180)
	        printf("YES");
	        else
	        printf("NO");
	        
	        printf("\n");
	}
	
	return 0;
}


