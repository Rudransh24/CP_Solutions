#include<stdio.h>


int main(void) {
	int t;
	scanf("%d",&t);
	unsigned long long int l;
	for(int i=0;i<t;i++)
	{
	   char str[100002];
	   int count1=0,count0=0;
	   scanf("%s",str);
	   l = strlen(str);
	   for(int i=0;i<l;i++)
	   {
	       if(str[i]=='1')
	        count1++;
	       if(str[i]=='0')
	        count0++;
	   }

	   if(count1==1 || count0==1)
	    printf("Yes\n");
	   else
	    printf("No\n");

	}
	return 0;
}



